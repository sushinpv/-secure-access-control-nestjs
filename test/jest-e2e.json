{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@secure-access-control/(.*)": "<rootDir>/../libs/secure-access-control/src/$1",
    "@secure-access-control": "<rootDir>/../libs/secure-access-control/src"
  }
}